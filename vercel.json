{
    "routes": [
      {
        "src": "/404",
        "dest": "/error.html" 
      },
      {
        "src": "/(.*)",
        "dest": "/$1"  
      }
    ],
    "cleanUrls": true,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, s-maxage=3600"
          }
        ]
      }
    ]
  }
  